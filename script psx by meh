local ScreenGui = Instance.new("ScreenGui")
local main = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local PETSIMXGUI = Instance.new("TextButton")
local PETSIMXFAKEHATCHER = Instance.new("TextButton")
local PETSIMXFAKEEQUIP = Instance.new("TextButton")
local CMDXADMIN = Instance.new("TextButton")


ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

main.Name = "main"
main.Parent = ScreenGui
main.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
main.Position = UDim2.new(0.203336805, 0, 0.252032518, 0)
main.Size = UDim2.new(0, 597, 0, 305)
main.Active = true
main.Draggable = true

TextLabel.Parent = main
TextLabel.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
TextLabel.Size = UDim2.new(0, 597, 0, 50)
TextLabel.Font = Enum.Font.SourceSans
